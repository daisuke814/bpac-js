    "dev": "tsup --watch",
    "type-check": "tsc",
    "test": "mocha",
    "version": "npx changeset"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeasir01/bpac-js.git"
